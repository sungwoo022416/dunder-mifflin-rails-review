<%= form_for @employee do |f| %>
    <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    </div>

    <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    </div>

    <div class="field">
    <%= f.label :alias %>
    <%= f.text_field :alias %>
    </div>

    <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
    </div>

    <div class="field">
    <%= f.label :office %>
    <%= f.text_field :office %>
    </div>

    <div class="field">
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
    </div>

    <div class="field">
    <%= f.label :dog_id %>
    <%= f.collection_select :dog_id, Dog.all, :id, :name %>
    </div>

    <div class="actions">
    <%= f.submit %>
    </div>
<% end %>